@model IEnumerable<THLab1.Models.Character>
@{
    // var u = Model;
}


<ul class="mt-5 mb-5 pe-5 ps-5 col">
    @foreach (var c in Model)
    {
        <div class="container g-3 col-md-6 border p-3 rounded">
            <div class="d-flex align-items-center">
                <image id="charPic" src="@c.charPicUrl"></image>
                <div class="mt-3 mb-3">
                    <li>Name: @c.name</li>
                    <li>Strength: @c.strength</li>
                    <li>Agility: @c.agility</li>
                    <li>Something: @c.something</li>
                </div>
            </div>
        </div>
    }
</ul>